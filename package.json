{
  "name": "image-magician",
  "version": "0.4.0",
  "description": "Provide some common image manipulation apis",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijinke666/image-magician.git"
  },
  "homepage": "https://lijinke666.github.io/image-magician",
  "bugs": {
    "url": "https://github.com/lijinke666/image-magician/issues"
  },
  "main": "lib/index.js",
  "author": "Jinke.Li <1359518268@qq.com>",
  "license": "MIT",
  "keywords": [
    "photo",
    "npm",
    "cover",
    "image",
    "images",
    "filter",
    "image-magician",
    "magician",
    "clip",
    "compress"
  ],
  "scripts": {
    "start": "http-server",
    "clean": "rimraf lib",
    "minify": "uglifyjs lib/index.js -o lib/index.min.js  --compress --mangle",
    "build:node": "babel src -d lib",
    "build:browser": "browserify src/index.js -o lib/index.browser.js -t [ babelify --presets [ \"babel-preset-env\" ] ]",
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "test": "jest __tests__/index.test",
    "pub": "npm run clean && npm run build",
    "precommit": "lint-staged",
    "lint": "prettier --write \"src/**/*.js\" && eslint_d --fix src",
    "upgrade": "yarn upgrade",
    "prepublish": "npm run pub"
  },
  "pre-commit": "lint",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.x",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "6.x",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint_d": "^5.3.0",
    "jest": "^22.4.3",
    "jsdom": "^11.8.0",
    "lint-staged": "^7.0.4",
    "power-assert": "^1.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      ".",
      "src",
      "src/shared"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "jsdom"
  }
}
