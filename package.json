{
  "name": "image-magician",
  "version": "0.4.0",
  "description": "Provide some common image manipulation apis",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijinke666/image-magician.git"
  },
  "homepage": "https://lijinke666.github.io/image-magician",
  "bugs": {
    "url": "https://github.com/lijinke666/image-magician/issues"
  },
  "main": "lib/index.js",
  "author": "Jinke.Li <1359518268@qq.com>",
  "license": "MIT",
  "keywords": [
    "photo",
    "npm",
    "cover",
    "image",
    "images",
    "filter",
    "image-magician",
    "magician",
    "clip",
    "compress"
  ],
  "scripts": {
    "start": "http-server",
    "clean": "rimraf lib",
    "minify": "uglifyjs lib/imageMagician.js -o lib/imageMagician.min.js  --compress --mangle",
    "build:node": "babel src -d lib",
    "build:browser": "browserify src/imageMagician.js -o lib/imageMagician.browser.js -t [ babelify --presets [ \"babel-preset-env\" ] ]",
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "test": "mocha -t 20000 --require intelli-espower-loader ./__tests__",
    "pub": "npm run clean && npm run build",
    "update": "yarn upgrade",
    "prepublish": "npm run pub"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.x",
    "babel-jest": "^19.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "6.x",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "rimraf": "^2.6.1"
  }
}
